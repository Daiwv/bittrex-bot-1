{
  "name": "bittrex-bot",
  "version": "1.0.0",
  "description": "A bot to trade on bittrex",
  "repository": {
    "git": "https://github.com/geopan/bittrex-bot.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha ./src/*.spec.js",
    "cover": "nyc mocha ./src/*.spec.js; nyc report --reporter=lcov",
    "coveralls":
      "cat ./coverage/lcov-report/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v"
  },
  "keywords": ["bittrex", "bot", "trading"],
  "author": "geopan",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1"
  },
  "engines": {
    "node": ">=8"
  },
  "nyc": {
    "exclude": ["**/*.spec.js"]
  }
}
